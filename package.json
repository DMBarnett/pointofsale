{
  "name": "pointofsale",
  "version": "1.0.0",
  "description": "Retail or restaurant  POS",
  "main": "server.js",
  "scripts": {
    "client": "npm start --prefix client",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "server": "nodemon server.js",
    "start:dev": "concurrently \"npm run server\" \"npm run client\"",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DMBarnett/pointofsale.git"
  },
  "author": "DMBarnett",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DMBarnett/pointofsale/issues"
  },
  "homepage": "https://github.com/DMBarnett/pointofsale#readme",
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "concurrently": "^4.1.0",
    "express": "^4.16.4",
    "mysql": "^2.16.0",
    "mysql2": "^1.6.4",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "passport-local": "^1.0.0",
    "safe-json-stringify": "^1.2.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"
  }
}
